<?xml version="1.0" encoding="utf-8"?>
<Application
    x:Class="Adita.PlexNet.WinUI.Dialogs.Sample.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Adita.PlexNet.WinUI.Dialogs.Sample"
    xmlns:dialogs="using:Adita.PlexNet.WinUI.Dialogs">
    <Application.Resources>
        <ResourceDictionary>
            
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->
            <dialogs:DialogView.ViewTemplates>
                <dialogs:DialogViewTemplate TargetType="dialogs:DialogHostProvider" ViewTemplate="{StaticResource Test}"/>
            </dialogs:DialogView.ViewTemplates>
        </ResourceDictionary>
    </Application.Resources>
</Application>
